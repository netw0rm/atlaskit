    "karma-webpack": "^1.7.0",
    "lerna": "jpnelson/lerna#config-concurrency-committed-lib-with-exec",
    "lerna-semantic-release": "^1.0.5",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "1.2.0",
    "mocha": "^2.4.5",
    "precommit-hook": "^3.0.0",
    "raw-loader": "^0.5.1",
    "semantic-release": "6.2.2",
