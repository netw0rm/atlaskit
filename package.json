  "engines": {
    "npm": ">=3.8",
    "node": ">=6.1"
  },
  "homepage": "https://aui-cdn.atlassian.com/atlaskit/registry/",
  "keywords": [
    "atlassian",
    "components",
    "interface",
    "ui",
    "web"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "os": [
    "!win32"
  ],
  "pre-commit": [
    "validate/lint-changed"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "scripts": {
    "clean": "./node_modules/.bin/lerna exec -- rm -rf node_modules dist && rm -rf node_modules",
    "commit": "git-cz -n",
    "create": "./build/bin/create.new.component.sh",
    "cucumber/single": "./build/bin/cucumber.single.sh",
    "dist": "./node_modules/.bin/lerna run prepublish",
    "docs/single": "./build/bin/docs.single.sh",
    "postinstall": "./build/bin/postinstall.sh",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' . --ext .js,.jsx",
    "pr/storybook": "./build/bin/storybook.static.pr.sh",
    "prepublish/single": "./build/bin/prepublish.single.sh",
    "prune": "./node_modules/.bin/lerna exec -- npm prune && npm prune",
    "release": "git diff --exit-code && lerna-semantic-release pre && lerna-semantic-release perform",
    "release/announce": "[ -s .released-packages ] && cat .released-packages | sed 's/^/Released /' | xargs ./build/bin/hipchat_message.sh || return 0",
    "release/changelog": "if [ -s .released-packages ]; then lerna-semantic-release post; else return 0; fi",
    "release/registry": "./build/bin/registry.sh",
    "shrinkwrap/update": "npm run clean && rm npm-shrinkwrap.json && npm install --ignore-scripts && npm shrinkwrap && npm run postinstall",
    "storybook": "start-storybook -p 9001 -c ./build/storybook",
    "storybook/single": "./build/bin/storybook.single.sh",
    "storybook/static": "build-storybook -c ./build/storybook",
    "storybook/static/single": "./build/bin/storybook.static.single.sh",
    "storybook/static/registry": "rm -rf ./stories && ./node_modules/.bin/lerna exec --concurrency=1 -- ../../build/bin/storybook.static.registry.sh",
    "test": "karma start karma.conf.js --single-run --no-auto-watch --reporters=dots",
    "test/base": "mocha",
    "test/browserstack": "./build/bin/test.browserstack.sh",
    "test/browserstack/ci": "./build/bin/test.browserstack.ci.sh",
    "test/integration/browserstack": "node ./build/bin/test.integration.browserstack.js",
    "test/single": "./build/bin/test.single.sh",
    "test/single/integration": "./build/bin/test.single.integration.sh",
    "test/single/integration/browserstack": "node ./build/bin/test.single.integration.browserstack.js",
    "test/single/browserstack": "./build/bin/test.single.browserstack.sh",
    "test/single/watch": "./build/bin/test.single.watch.sh",
    "validate": "npm ls >/dev/null && ./node_modules/.bin/lerna exec -- npm ls >/dev/null",
    "validate/lint-changed": "./build/bin/lint-changed.sh",
    "validate/package.json": "pjv && ./node_modules/.bin/lerna exec -- pjv",
    "webpack/single": "./build/bin/webpack.single.sh"
  }
}
