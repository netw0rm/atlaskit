  "license": "Apache-2.0",
  "main": "index.js",
  "os": [
    "!win32"
  ],
  "pre-commit": [
    "validate/lint-changed"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "scripts": {
    "build/docker/image": "build/bin/build.docker.image.sh",
    "browserstack/tunnel/start": "./build/bin/browserstack.tunnel.start.sh",
    "browserstack/tunnel/stop": "./build/bin/browserstack.tunnel.stop.sh",
    "clean": "./node_modules/.bin/lerna exec -- rm -rf node_modules dist && rm -rf node_modules",
    "commit": "git-cz -n",
    "create": "./build/bin/create.new.component.sh",
    "cucumber/single": "./build/bin/cucumber.single.sh",
    "dist": "./node_modules/.bin/lerna run prepublish",
    "docs": "./node_modules/.bin/lerna exec -- ../../build/bin/generate.readme.sh",
    "docs/single": "./build/bin/docs.single.sh",
    "postinstall": "./build/bin/postinstall.sh",
    "lint": "./build/bin/lint.sh",
    "pr/artifacts": "run-p -ln 'pr/docs' 'pr/storybook' 'pr/stats'",
    "pr/docs": "npm run docs && ./build/bin/generate.readme.html.pr.sh",
    "pr/storybook": "./build/bin/storybook.static.pr.sh",
    "pr/stats": "./build/bin/pr.stats.sh",
    "prepublish/single": "./build/bin/prepublish.single.sh",
    "prune": "./node_modules/.bin/lerna exec -- npm prune && npm prune",
    "release": "lerna-semantic-release pre && npm run docs && lerna-semantic-release post && lerna-semantic-release perform",
    "release/announce": "./build/bin/release.announce.js || return 0",
    "release/check/is-workspace-clean": "git diff --exit-code || (chalk --no-stdin -t \"{red \n^ Workspace is not clean!}\"; exit 1)",
    "release/check/can-ff": "git pull --ff-only --no-edit origin || (chalk --no-stdin -t \"{blue We are not the leading build - nothing to do here!}\"; exit 1)",
    "release/registry": "./build/bin/release.registry.sh || (chalk --no-stdin -t \"{red Registry generation failed!}\")",
    "release/storybooks": "./build/bin/release.storybooks.sh || (chalk --no-stdin -t \"{red Storybook generation failed!}\")",
    "shrinkwrap/update": "npm run clean && rm -f npm-shrinkwrap.json && npm install && npm shrinkwrap",
    "storybook": "start-storybook --dont-track -p 9001 -c ./build/storybook",
    "storybook/single": "./build/bin/storybook.single.sh",
    "storybook/static": "build-storybook -c ./build/storybook",
    "storybook/static/single": "./build/bin/storybook.static.single.sh",
    "storybook/static/registry": "rm -rf ./stories && ./node_modules/.bin/lerna exec --concurrency=1 -- ../../build/bin/storybook.static.registry.sh",
    "storybook/wait": "wait-on -d 20000 -i 500 -t 300000 http-get://0.0.0.0:9001/",
    "test": "karma start ./build/karma/all.js --single-run --no-auto-watch --reporters=dots",
    "test/watch": "NODE_ENV=development karma start ./build/karma/all.js --reporters=dots",
    "test/base": "mocha --colors",
    "test/browserstack": "./build/bin/test.browserstack.sh",
    "test/integration": "./build/bin/test.integration.sh",
    "test/integration/browserstack": "./node_modules/.bin/lerna exec --concurrency 1 -- ../../build/bin/test.integration.browserstack.sh",
    "test/integration/browserstack/wait": "npm run storybook/wait && npm run test/integration/browserstack",
    "test/single": "./build/bin/test.single.sh",
    "test/single/integration": "./build/bin/test.single.integration.sh",
    "test/single/integration/browserstack": "./build/bin/test.single.integration.browserstack.sh",
    "test/single/browserstack": "./build/bin/test.single.browserstack.sh",
    "test/single/watch": "NODE_ENV=development ./build/bin/test.single.watch.sh",
    "validate": "npm ls >/dev/null && ./node_modules/.bin/lerna exec -- npm ls >/dev/null",
    "validate/lint-changed": "./build/bin/lint-changed.sh",
    "validate/package.json": "pjv -q && ./node_modules/.bin/lerna exec -- pjv -q"
  }
}
