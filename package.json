    "skatejs": "0.14.2"
  },
  "engines": {
    "npm": ">=3.8",
    "node": ">=6.1"
  },
  "homepage": "http://aui.atlassian.com",
  "keywords": [
    "atlassian",
    "components",
    "interface",
    "ui",
    "web"
  ],
  "license": "Proprietary",
  "main": "index.js",
  "os": [
    "!win32"
  ],
  "pre-commit": [
    "lint"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:atlassian/atlaskit-spike.git"
  },
  "scripts": {
    "clean": "./node_modules/.bin/lerna exec -- rm -rf node_modules dist && rm -rf node_modules",
    "dev/single": "./build/bin/dev.single.sh",
    "dist": "./node_modules/.bin/lerna run prepublish",
    "lint": "eslint .",
    "postinstall": "./node_modules/.bin/lerna bootstrap && validate-commit-msg",
    "prepublish/single": "./build/bin/prepublish.single.sh",
    "release": "lerna-semantic-release pre && lerna-semantic-release perform",
    "release/announce": "[ -s .released-packages ] && cat .released-packages | sed 's/^/Released /' | xargs ./build/bin/hipchat_message.sh || return 0",
    "release/changelog": "if [ -s .released-packages ]; then lerna-semantic-release post; else return 0; fi",
    "test": "./build/bin/test.sh",
    "test/saucelabs": "./build/bin/test.saucelabs.sh",
    "test/saucelabs/ci": "./build/bin/test.saucelabs.ci.sh",
    "test/single": "./build/bin/test.single.sh",
    "test/single/monkey": "./build/bin/test.single.monkey.sh",
    "test/single/saucelabs": "./build/bin/test.single.saucelabs.sh",
    "test/single/watch": "./build/bin/test.single.watch.sh",
    "validate": "npm ls",
    "webpack/single": "./build/bin/webpack.single.sh"
  }
}
