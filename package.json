{
  "name": "atlaskit",
  "version": "0.0.0",
  "author": "Atlassian Pty Ltd",
  "config": {
    "commitizen": {
      "path": "./packages/akutil-cz-atlaskit-changelog"
    }
  },
  "dependencies": {
    "@kadira/storybook": "^1.38.0",
    "ak-dmd-plugin": "0.0.7",
    "akutil-polyfills": "^0.0.11",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-incremental-dom": "^3.4.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "browserstack-local": "1.0.0",
    "camelcase": "^3.0.0",
    "commitizen": "^2.8.2",
    "css-loader": "^0.23.1",
    "cucumber": "^1.2.1",
    "cz-conventional-changelog": "^1.1.6",
    "cz-lerna-changelog": "0.2.2",
    "dmd-bitbucket": "0.1.9",
    "eslint": "^2.13.1",
    "eslint-config-ak-base": "^1.1.2",
    "eslint-config-ak-cucumber": "^1.0.2",
    "eslint-config-ak-stories": "^2.0.0",
    "eslint-config-ak-tests": "^1.0.2",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-import": "^1.11.0",
    "file-loader": "^0.9.0",
    "gremlins.js": "marmelab/gremlins.js#699af579a2f3e36cd3ba1d68c13893e80e40e674",
    "jsdoc-to-markdown": "^1.3.6",
    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-browserstack-launcher": "^1.0.1",
    "karma-chai-plugins": "^0.7.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lerna": "2.0.0-beta.24",
    "lerna-semantic-release": "^4.0.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minilog": "^3.0.1",
    "minimist": "1.2.0",
    "mocha": "^2.5.3",
    "pascal-case": "^2.0.0",
    "precommit-hook": "^3.0.0",
    "protractor": "^3.3.0",
    "protractor-console": "^2.0.1",
    "protractor-cucumber-framework": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "retry-cli": "joscha/retry-cli#8c581f9",
    "semantic-release": "~4.3.0",
    "stats.js": "^0.16.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "validate-commit-message": "^3.0.1",
    "wait-on": "^1.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "npm": ">=3.8",
    "node": ">=6.1"
  },
  "homepage": "http://aui.atlassian.com",
  "keywords": [
    "atlassian",
    "components",
    "interface",
    "ui",
    "web"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "os": [
    "!win32"
  ],
  "pre-commit": [
    "validate/lint-changed"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "scripts": {
    "clean": "./node_modules/.bin/lerna exec -- rm -rf node_modules dist && rm -rf node_modules",
    "commit": "git-cz -n",
    "create": "./build/bin/create.new.component.sh",
    "cucumber/single": "./build/bin/cucumber.single.sh",
    "dist": "./node_modules/.bin/lerna run prepublish",
    "docs/single": "./build/bin/docs.single.sh",
    "postinstall": "./node_modules/.bin/lerna bootstrap && validate-commit-msg && npm run lerna/link",
    "lerna/link": "./build/bin/no.js.npm.link.sh",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' . --ext .js,.jsx",
    "prepublish/single": "./build/bin/prepublish.single.sh",
    "prune": "./node_modules/.bin/lerna exec -- npm prune && npm prune",
    "release": "git diff --exit-code && lerna-semantic-release pre && lerna-semantic-release perform",
    "release/announce": "[ -s .released-packages ] && cat .released-packages | sed 's/^/Released /' | xargs ./build/bin/hipchat_message.sh || return 0",
    "release/changelog": "if [ -s .released-packages ]; then lerna-semantic-release post; else return 0; fi",
    "release/registry": "./build/bin/registry.sh",
    "storybook": "start-storybook -p 9001 -c ./build/storybook",
    "storybook/single": "./build/bin/storybook.single.sh",
    "test": "./node_modules/.bin/lerna exec -- ../../build/bin/test.sh --reporters=dots",
    "test/ci": "./node_modules/.bin/lerna exec --concurrency=1 -- ../../build/bin/test.sh --no-auto-watch --reporters=dots",
    "test/browserstack": "./build/bin/test.browserstack.sh",
    "test/browserstack/ci": "./build/bin/test.browserstack.ci.sh",
    "test/integration/browserstack": "node ./build/bin/test.integration.browserstack.js",
    "test/single": "./build/bin/test.single.sh",
    "test/single/integration": "./build/bin/test.single.integration.sh",
    "test/single/integration/browserstack": "node ./build/bin/test.single.integration.browserstack.js",
    "test/single/browserstack": "./build/bin/test.single.browserstack.sh",
    "test/single/watch": "./build/bin/test.single.watch.sh",
    "validate": "npm ls >/dev/null && ./node_modules/.bin/lerna exec -- npm ls >/dev/null",
    "validate/lint-changed": "./build/bin/lint-changed.sh",
    "webpack/single": "./build/bin/webpack.single.sh"
  }
}
