    "cucumber": "^1.2.2",
    "cz-conventional-changelog": "^1.1.7",
    "cz-lerna-changelog": "0.3.0",
    "dmd-bitbucket": "0.1.10",
    "eslint": "^2.13.1",
    "eslint-config-ak-base": "^1.1.2",
    "eslint-config-ak-cucumber": "^1.0.2",
    "eslint-config-ak-stories": "^2.0.0",
