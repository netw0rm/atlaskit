{
  "name": "@atlaskit/editor-bitbucket",
  "version": "1.0.0",
  "description": "Editor web component",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "ak:webpack:raw": "src/index.tsx",
  "types": "src/index.tsx",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && npm run build || ../../node_modules/.bin/not-in-publish",
    "build": "npm run build/es5 && npm run build/es2015 && npm run build/package.json",
    "build/es5": "../../node_modules/.bin/tsc -p build/es5",
    "build/es2015": "../../node_modules/.bin/tsc -p build/es2015",
    "build/package.json": "../../build/bin/overlay-package-json.js package.publish.json package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "Apache-2.0",
  "author": "Atlassian Pty Ltd",
  "dependencies": {
    "ak-editor-core": "^3.10.1",
    "ak-editor-shared-styles": "^3.0.0",
    "ak-mention": "^1.9.0",
    "akutil-shared-styles": "^37.0.1",
    "tslib": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "ak-mention": "*",
    "akutil-common-test": "*",
    "js-search": "^1.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "atlaskit",
    "ui"
  ]
}
