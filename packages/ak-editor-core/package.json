{
  "name": "ak-editor-core",
  "version": "3.10.8",
  "description": "A package contains Atlassian editor core functionality",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "ak:webpack:raw": "src/index.ts",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && npm run build || ../../node_modules/.bin/not-in-publish",
    "build": "npm run build/es5 && npm run build/es2015 && npm run build/prosemirror",
    "build/es5": "../../node_modules/.bin/babel --out-dir dist/es5 --source-root src --source-maps true --no-babelrc --plugins transform-runtime --presets es2015 src && ../../node_modules/.bin/tsc -p build/es5",
    "build/es2015": "../../node_modules/.bin/babel --out-dir dist/es2015 --source-root src --source-maps true --no-babelrc src && ../../node_modules/.bin/tsc -p build/es2015",
    "build/prosemirror": "cp -rf src/prosemirror dist/types && find dist/types/prosemirror -name '*.js' -exec rm {} \\;"
  },
  "files": [
    "dist",
    "src",
    "test-helper"
  ],
  "license": "Apache-2.0",
  "author": "Atlassian Pty Ltd",
  "dependencies": {
    "ak-button": "^5.0.0",
    "ak-button-group": "^3.0.2",
    "ak-droplist": "^4.1.0",
    "ak-droplist-item": "^4.0.0",
    "ak-droplist-group": "^2.1.0",
    "ak-dropdown-menu": "^2.1.0",
    "ak-editor-shared-styles": "^3.2.0",
    "ak-icon": "^14.0.0",
    "ak-mention": "^1.9.0",
    "akutil-shared-styles": "^37.0.2",
    "browserkeymap": "git://github.com/bradleyayers/browserkeymap#es6-modules-compat-with-types",
    "popper.js": "^1.0.1",
    "prosemirror": "0.10.1",
    "tslib": "^1.2.0",
    "typestyle": "^0.21.4"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "ajv": "^4.7.2",
    "highlight.js": "^9.6.0",
    "sinon": "^1.17.6",
    "string.prototype.repeat": "^0.2.0",
    "thenable": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  }
}
