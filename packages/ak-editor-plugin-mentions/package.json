{
  "name": "ak-editor-plugin-mentions",
  "version": "5.3.0",
  "description": "This plugin provides mentions node type and related behaviors to Atlassian Editor",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "ak:webpack:raw": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && { ../../node_modules/.bin/tsc -p build/es5 && ../../node_modules/.bin/tsc -p build/es2015; } || ../../node_modules/.bin/not-in-publish"
  },
  "files": [
    "dist",
    "**/*.d.ts"
  ],
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "dependencies": {
    "ak-editor-prosemirror": "^4.0.0",
    "tslib": "^1.2.0"
  },
  "devDependencies": {
    "ak-editor-test": "*",
    "ak-editor-schema": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "atlaskit",
    "ui"
  ]
}
