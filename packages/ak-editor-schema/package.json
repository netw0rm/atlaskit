{
  "name": "ak-editor-schema",
  "version": "4.7.1",
  "description": "A common schema to be used in Atlassian Editor.",
  "jsnext:main": "dist/es2015/index.js",
  "main": "dist/es5/index.js",
  "ak:webpack:raw": "src/index.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && { ../../node_modules/.bin/tsc -p build/es5 && ../../node_modules/.bin/tsc -p build/es2015; } || ../../node_modules/.bin/not-in-publish"
  },
  "files": [
    "dist",
    "**/*.d.ts"
  ],
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "dependencies": {
    "ak-editor-prosemirror": "^4.3.14",
    "tslib": "^1.2.0"
  },
  "devDependencies": {
    "ajv": "^4.7.2",
    "ak-editor-bitbucket": "*",
    "ak-editor-test": "*",
    "ak-tabs": "*",
    "akutil-react": "*",
    "highlight.js": "^9.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "atlaskit",
    "ui"
  ]
}
