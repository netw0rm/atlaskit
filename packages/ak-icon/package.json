{
  "name": "ak-icon",
  "version": "8.4.0",
  "description": "Icon web components",
  "main": "dist/bundle.js",
  "webpack": "dist/bundle-cjs.js",
  "ak:webpack:raw": "src/index.js",
  "scripts": {
    "generate-bundle-exports": "node ./bin/generate-bundle-exports.js > ./src/index.js",
    "generate-icon-exports": "node ./bin/generate-icon-exports.js",
    "gen-js": "npm run generate-icon-exports && npm run generate-bundle-exports",
    "gen-icons-md": "node ./bin/generate-icons-md.js > ./docs/Icons.md",
    "prepublish": "CLI=1 npm run gen-js && npm run gen-icons-md && cd ../.. && npm run prepublish/single ak-icon",
    "pretest": "npm run gen-js"
  },
  "files": [
    "dist",
    "glyph"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": "Atlassian Pty Ltd",
  "dependencies": {
    "akutil-common": "^2.6.3",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "create-error": "^0.3.1",
    "skatejs": "^3.0.0"
  },
  "devDependencies": {
    "ak-button": "^1.4.3",
    "akutil-common-test": "^4.0.0",
    "akutil-react": "^1.0.0",
    "akutil-shared-styles": "^34.0.1",
    "async": "^2.0.1",
    "fs": "0.0.1-security",
    "glob": "^7.0.5",
    "lodash.sample": "^4.2.1",
    "mkdirp": "^0.5.1",
    "pascal-case": "^2.0.0",
    "rimraf": "^2.5.4",
    "svgo": "^0.7.0",
    "uid": "^0.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  }
}
