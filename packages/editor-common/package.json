{
  "name": "@atlaskit/editor-common",
  "version": "2.1.3",
  "config": {
    "access": "public"
  },
  "description": "A package that contains common classes and components for editor and renderer",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "module": "dist/es2015/index.js",
  "types": "dist/es5/index.d.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && npm run build || ../../node_modules/.bin/not-in-publish",
    "build": "npm run build/es5 && npm run build/es2015",
    "build/es5": "../../node_modules/.bin/tsc -p build/es5",
    "build/es2015": "../../node_modules/.bin/tsc -p build/es2015",
    "build/json-schema": "mkdir -p dist/json-schema/v1 && ../../node_modules/.bin/tsc -p build/json-schema && node ./dist/json-schema-cli/index.js ./src/schema/nodes/doc.ts > ./dist/json-schema/v1/full.json",
    "lint": "../../node_modules/.bin/tslint --project . '*.{ts,tsx,d.ts}' '{src,test,stories}/**/*.{ts,tsx,d.ts}'",
    "validate/typescript": "../../build/bin/validate.typescript.sh",
    "test:browser": "node --max-old-space-size=4096 ../../node_modules/.bin/karma start ../../build/config/karma/config.js",
    "test:unit": "../../node_modules/.bin/jest -c ../../build/config/jest.config.js"
  },
  "files": [
    "dist/es5/**/*.js",
    "dist/es5/**/*.d.ts",
    "dist/es2015/**/*.js",
    "dist/es2015/**/*.d.ts",
    "dist/json-schema",
    "src"
  ],
  "license": "Apache-2.0",
  "author": "Atlassian Pty Ltd",
  "maintainers": [
    {
      "name": "Oscar W"
    },
    {
      "name": "Stanislav S"
    }
  ],
  "dependencies": {
    "@atlaskit/emoji": "^30.1.1",
    "@atlaskit/icon": "^6.5.2",
    "@atlaskit/media-card": "^21.5.0",
    "@atlaskit/media-core": "^10.2.0",
    "@atlaskit/mention": "^7.3.4",
    "@atlaskit/profilecard": "^3.7.1",
    "@atlaskit/util-shared-styles": "^2.1.0",
    "@types/prosemirror-model": "^0.21.0",
    "@types/prosemirror-schema-basic": "^0.21.0",
    "@types/prosemirror-view": "^0.21.1",
    "ajv": "^4.11.3",
    "css-color-names": "0.0.4",
    "linkify-it": "^2.0.3",
    "prosemirror-model": "^0.22.0",
    "prosemirror-schema-basic": "^0.22.0",
    "prosemirror-tables": "^0.1.0",
    "prosemirror-view": "^0.22.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@atlaskit/media-test-helpers": "^8.4.0",
    "json-schema-diff-validator": "^0.0.4",
    "sinon": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "fabric",
    "ui"
  ]
}
