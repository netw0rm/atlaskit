{
  "name": "@atlaskit/editor-core",
  "version": "51.1.0",
  "config": {
    "access": "public"
  },
  "description": "A package contains Atlassian editor core functionality",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "module": "dist/es2015/index.js",
  "types": "dist/es5/index.d.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && npm run build || ../../node_modules/.bin/not-in-publish && npm run build/json-schema",
    "build": "../../build/bin/generate.version.export.js ./package.json src/version.ts && npm run build/es2015 && npm run build/es5 && npm run build/json-schema",
    "build/es2015": "../../node_modules/.bin/babel --out-dir dist/es2015 --source-root src --no-babelrc src && ../../node_modules/.bin/tsc -p build/es2015",
    "build/es5": "mkdir -p dist/es5 && cp -Rf src/prosemirror dist/es5/ && ../../node_modules/.bin/babel --out-dir dist/es5 --source-root src --no-babelrc --plugins transform-runtime --presets es2015 src && ../../node_modules/.bin/tsc -p build/es5",
    "build/json-schema": "mkdir -p dist/json-schema/v1 && ../../node_modules/.bin/tsc -p build/json-schema && node ./dist/json-schema-cli/index.js ./src/schema/nodes/doc.ts > ./dist/json-schema/v1/full.json",
    "lint": "../../node_modules/.bin/tslint --project . '*.{ts,tsx,d.ts}' '{src,test,stories}/**/*.{ts,tsx,d.ts}'",
    "validate/typescript": "../../build/bin/validate.typescript.sh",
    "storybook": "../../build/bin/storybook/run.local.js",
    "storybook:static": "../../build/bin/storybook/build.static.js",
    "test:browser": "node --max-old-space-size=4096 ../../node_modules/.bin/karma start ../../build/config/karma/config.js",
    "test:unit": "../../node_modules/.bin/jest -c ../../build/config/jest.config.js"
  },
  "files": [
    "dist/es5/**/*.js",
    "dist/es5/**/*.d.ts",
    "dist/es2015/**/*.js",
    "dist/es2015/**/*.d.ts",
    "dist/json-schema",
    "src"
  ],
  "license": "Apache-2.0",
  "author": "Atlassian Pty Ltd",
  "maintainers": [
    {
      "name": "Eduard S"
    },
    {
      "name": "Artur B"
    },
    {
      "name": "Huanhuan H"
    },
    {
      "name": "Oscar W"
    }
  ],
  "dependencies": {
    "@atlaskit/activity": "^1.0.1",
    "@atlaskit/avatar": "^8.0.0",
    "@atlaskit/button": "^5.1.1",
    "@atlaskit/code": "^2.1.2",
    "@atlaskit/dropdown-menu": "^1.7.0",
    "@atlaskit/droplist": "^3.8.1",
    "@atlaskit/emoji": "^29.0.0",
    "@atlaskit/icon": "^6.5.2",
    "@atlaskit/layer": "^2.5.4",
    "@atlaskit/logo": "^3.5.3",
    "@atlaskit/media-card": "^21.3.1",
    "@atlaskit/media-filmstrip": "^5.3.0",
    "@atlaskit/media-core": "^10.1.0",
    "@atlaskit/mention": "^7.3.4",
    "@atlaskit/profilecard": "^3.7.1",
    "@atlaskit/renderer": "^7.0.0",
    "@atlaskit/single-select": "^1.10.0",
    "@atlaskit/spinner": "^1.0.1",
    "@atlaskit/task-decision": "^4.0.0",
    "@atlaskit/tooltip": "^1.2.0",
    "@atlaskit/util-shared-styles": "^2.1.0",
    "@types/codemirror": "^0.0.40",
    "@types/orderedmap": "^1.0.0",
    "ajv": "^4.11.3",
    "codemirror": "^5.26.0",
    "collapse-whitespace": "^1.1.6",
    "css-color-names": "0.0.4",
    "linkify-it": "^2.0.3",
    "markdown-it-table": "^1.0.0",
    "mediapicker": "^9.1.0",
    "orderedmap": "^1.0.0",
    "prosemirror-commands": "^0.24.0",
    "prosemirror-history": "^0.24.0",
    "prosemirror-inputrules": "^0.24.0",
    "prosemirror-keymap": "^0.24.0",
    "prosemirror-markdown": "^0.24.0",
    "prosemirror-model": "^0.24.0",
    "prosemirror-schema-basic": "^0.24.0",
    "prosemirror-schema-list": "^0.24.0",
    "prosemirror-state": "^0.24.0",
    "prosemirror-tables": "^0.1.0",
    "prosemirror-transform": "^0.24.0",
    "prosemirror-view": "^0.24.0",
    "styled-components": "^1.4.6",
    "tslib": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "@atlaskit/button": "^5.1.1",
    "@atlaskit/media-test-helpers": "^8.3.0",
    "@atlaskit/util-data-test": "^7.0.0",
    "chai-as-promised": "^6.0.0",
    "fetch-mock": "^5.12.1",
    "highlight.js": "^9.6.0",
    "jquery": "^2.2.4",
    "js-search": "^1.3.3",
    "json-schema-diff-validator": "^0.0.4",
    "pretty-data": "^0.40.0",
    "prosemirror-dev-tools": "^1.3.3",
    "sinon": "^2.2.0",
    "string.prototype.repeat": "^0.2.0",
    "thenable": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "fabric",
    "ui"
  ]
}
