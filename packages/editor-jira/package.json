{
  "name": "@atlaskit/editor-jira",
  "version": "1.2.0",
  "config": {
    "access": "public"
  },
  "description": "An Atlassian Editor component specially crafted for JIRA.",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && npm run build || ../../node_modules/.bin/not-in-publish",
    "build": "npm run build/es5 && npm run build/es2015",
    "build/es5": "../../node_modules/.bin/tsc -p build/es5",
    "build/es2015": "../../node_modules/.bin/tsc -p build/es2015"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "Apache-2.0",
  "author": "Atlassian Pty Ltd",
  "dependencies": {
    "@atlaskit/editor-core": "^1.0.0",
    "ak-editor-shared-styles": "^3.0.0",
    "akutil-shared-styles": "^37.0.1",
    "collapse-whitespace": "^1.1.4",
    "tslib": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "@atlaskit/editor-jira": "*",
    "@atlaskit/util-readme": "^1.0.0",
    "akutil-common-test": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  }
}
