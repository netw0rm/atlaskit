{
  "name": "ak-base",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-plusplus": 0,
    "no-param-reassign": ["error", { "props": false }],
    "react/no-unknown-property": [0]
  },
  "settings": {
    "react": {
      "pragma": "vdom"
    }
  }
}
