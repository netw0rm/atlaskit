    "lint": "../../node_modules/.bin/tslint --project . '*.{ts,tsx,d.ts}' '{src,stories}/**/*.{ts,tsx,d.ts}'",
    "prepublish": "CLI=1 npm run gen-js && ../../node_modules/.bin/nwb build",
    "pretest": "npm run gen-js",
    "storybook": "../../node_modules/.bin/start-storybook -c ../../build/storybook-nwb -p 9001",
    "test": "../../node_modules/.bin/nwb test",
    "test:watch": "../../node_modules/.bin/nwb test --server"
  },
