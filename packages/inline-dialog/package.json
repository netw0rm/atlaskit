{
  "name": "@atlaskit/inline-dialog",
  "version": "5.0.4",
  "config": {
    "access": "public"
  },
  "description": "An inline dialog React component for secondary content and controls that are displayed on user request",
  "main": "dist/bundle.js",
  "webpack": "dist/bundle-cjs.js",
  "jsnext:experimental:main": "dist/es/index.js",
  "ak:webpack:raw": "src/index.jsx",
  "ak:component": {
    "name": "Inline Dialog",
    "dark": true
  },
  "scripts": {
    "eslint": "../../build/bin/lint.eslint.sh",
    "jest": "echo 'yarn run jest is deprecated and removed. please run yarn run test:unit instead.'",
    "prepublish": "../../node_modules/.bin/in-publish && { cd ../.. && npm run prepublish-with-babel/single @atlaskit/inline-dialog; } || ../../node_modules/.bin/not-in-publish",
    "storybook": "../../build/bin/storybook/run.local.js",
    "storybook:static": "../../build/bin/storybook/build.static.js",
    "test:unit": "../../node_modules/.bin/jest -c ../../build/config/jest.config.js"
  },
  "files": [
    "dist"
  ],
  "author": "Atlassian Pty Ltd",
  "maintainers": [
    {
      "name": "Luke B"
    },
    {
      "name": "Nadia M"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@atlaskit/layer": "^2.0.0",
    "@atlaskit/theme": "^2.0.0",
    "babel-runtime": "^6.11.6",
    "prop-types": "^15.5.10",
    "styled-components": "^1.4.6"
  },
  "devDependencies": {
    "@atlaskit/button": "^5.1.1",
    "@atlaskit/inline-dialog": "*",
    "@atlaskit/icon": "^8.1.0",
    "@atlaskit/single-select": "*"
  },
  "peerDependencies": {
    "react": "^15.3.2 || ^16.0.0",
    "react-dom": "^15.3.2 || ^16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
