{
  "name": "@atlaskit/media-editor",
  "version": "3.1.0",
  "config": {
    "access": "public"
  },
  "description": "Media component for image annotations",
  "main": "umd/@atlaskit/media-editor.js",
  "types": "umd/types/index.d.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint --project . '*.{ts,tsx,d.ts}' '{src,stories}/**/*.{ts,tsx,d.ts}'",
    "prepublish": "../../node_modules/.bin/nwb build && ../../build/bin/copy_vendored_dts.js",
    "storybook": "PACKAGE=media-editor ../../node_modules/.bin/start-storybook -c ../../build/storybook -p 9001",
    "test": "node --max-old-space-size=4096 ../../node_modules/.bin/nwb test",
    "test:watch": "../../node_modules/.bin/nwb test --server",
    "storybook:static": "../../build/bin/storybook/build-static.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@atlaskit/icon": "^5.0.0",
