    "@atlaskit/media-core": "^8.0.0",
    "react": "^15.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "devDependencies": {
    "@atlaskit/media-core": "^8.0.0",
