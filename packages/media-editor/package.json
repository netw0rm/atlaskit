{
  "name": "@atlaskit/media-editor",
  "version": "3.2.0",
  "config": {
    "access": "public"
  },
  "description": "Media component for image annotations",
  "main": "dist/es5/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "types": "dist/es5/index.d.ts",
  "scripts": {
    "prepublish": "../../node_modules/.bin/in-publish && npm run build || ../../node_modules/.bin/not-in-publish",
    "build": "npm run build/es5 && npm run build/es2015",
    "build/es5": "../../node_modules/.bin/tsc -p build/es5 && rm -rf dist/es5/engine/core/binaries && cp -r src/engine/core/binaries dist/es5/engine/core",
    "build/es2015": "../../node_modules/.bin/tsc -p build/es2015 && rm -rf dist/es2015/engine/core/binaries && cp -r src/engine/core/binaries dist/es2015/engine/core",
    "jest": "echo 'yarn run jest is deprecated and removed. please run yarn run test:unit instead.'",
    "lint": "../../node_modules/.bin/tslint --project . '*.{ts,tsx,d.ts}' '{src,test,stories}/**/*.{ts,tsx,d.ts}'",
    "storybook": "../../build/bin/storybook/run.local.js",
    "storybook:static": "../../build/bin/storybook/build.static.js",
    "test:unit": "../../node_modules/.bin/jest -c ../../build/config/jest.config.js",
    "validate/typescript": "../../build/bin/validate.typescript.sh"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@atlaskit/icon": "^5.0.0",
    "@atlaskit/util-shared-styles": "^1.0.0",
    "styled-components": "^1.3.0",
    "tslib": "^1.2.0"
  },
  "author": "Atlassian Pty. Ltd",
  "maintainers": [
    {
      "name": "Vadim K"
    }
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "@atlaskit/media-core": "^8.0.0 || ^9.0.0 || ^10.1.0 || ^^11.0.0",
    "react": "^15.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "devDependencies": {
    "@atlaskit/media-core": "^11.0.0",
    "akutil-common-test": "*"
  },
  "keywords": [
    "media",
    "ui"
  ]
}
