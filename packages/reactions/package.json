    "@atlaskit/tooltip": "^1.2.0",
    "@atlaskit/util-shared-styles": "^1.0.0",
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "react-transition-group": "^1.1.3",
    "tslib": "^1.2.0",
    "typestyle": "^0.21.4",
    "whatwg-fetch": "^1.0.0"
  },
