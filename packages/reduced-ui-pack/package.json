{
  "name": "@atlaskit/reduced-ui-pack",
  "version": "5.3.2",
  "config": {
    "access": "public"
  },
  "description": "CSS classes which provide styling for a reduced number of AtlasKit components.",
  "main": "dist/bundle.css",
  "scripts": {
    "lint": "../../node_modules/.bin/eslint --color --format \"../../node_modules/eslint-friendly-formatter\" --ext .js --ext .jsx src/ stories/ test/",
    "prepublish": "lessc src/index.less dist/bundle.css && node ./bin/buildSprite.js",
    "pretest": "npm run prepublish",
    "storybook": "../../node_modules/.bin/start-storybook -c ../../build/storybook-nwb -p 9001",
    "test": "../../node_modules/.bin/nwb test",
    "test:watch": "../../node_modules/.bin/nwb test --server"
  },
  "files": [
    "umd"
  ],
  "dependencies": {
    "@atlaskit/util-shared-styles": "^1.0.0"
  },
  "peerDependencies": {
    "@atlaskit/css-reset": "^1.0.0"
  },
  "author": "Atlassian Pty Ltd",
  "maintainers": [
    {
      "name": "Sean C"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@atlaskit/field-text": "^2.0.5",
    "@atlaskit/util-readme": "^1.0.0",
    "cheerio": "^0.22.0",
    "glob": "^7.1.1",
    "less": "^2.7.1",
    "mkdirp": "^0.5.1",
    "svg-sprite": "^1.3.6",
    "uid": "0.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "atlaskit",
    "ui"
