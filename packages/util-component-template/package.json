{
  "name": "@atlaskit/util-component-template",
  "version": "2.0.9",
  "config": {
    "access": "public"
  },
  "description": "A template for creating AtlasKit components",
  "main": "umd/@atlaskit/util-component-template.js",
  "scripts": {
    "lint": "../../node_modules/.bin/eslint --color --format \"../../node_modules/eslint-friendly-formatter\" --ext .js --ext .jsx src/ stories/ test/",
    "prepublish": "../../node_modules/.bin/nwb build",
    "storybook": "../../node_modules/.bin/start-storybook -c ../../build/storybook-nwb -p 9001",
    "test": "mocha --colors --require ../../test-setup './test/unit/**/*.js' --timeout 10000",
    "test:watch": "mocha --watch --colors --require ../../test-setup './test/unit/**/*.js' --timeout 10000"
  },
  "files": [
    "umd"
  ],
  "dependencies": {
    "@atlaskit/util-shared-styles": "^1.0.1",
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "@atlaskit/util-readme": "^3.0.0",
    "@atlaskit/util-component-template": "*"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "styled-components": "^1.3.0"
  },
  "author": "Atlassian Pty Ltd",
  "maintainers": [],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "keywords": [
    "atlaskit",
    "ui",
    "internal"
  ]
}