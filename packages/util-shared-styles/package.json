{
  "name": "@atlaskit/util-shared-styles",
  "version": "3.0.0",
  "config": {
    "access": "public"
  },
  "description": "Shared styles for Atlaskit components",
  "main": "dist/bundle.js",
  "webpack": "dist/bundle-cjs.js",
  "ak:webpack:raw": "src/index.js",
  "scripts": {
    "build": "node ./bin/generateJSVarsFromLess && { cd ../.. && npm run prepublish/single @atlaskit/util-shared-styles; }",
    "eslint": "../../build/bin/lint.eslint.sh",
    "jest": "echo 'yarn run jest is deprecated and removed. please run yarn run test:unit instead.'",
    "prepublish": "node ./bin/generateJSVarsFromLess && ../../node_modules/.bin/in-publish && { cd ../.. && npm run prepublish/single @atlaskit/util-shared-styles; } || ../../node_modules/.bin/not-in-publish",
    "storybook": "../../build/bin/storybook/run.local.js",
    "storybook:static": "../../build/bin/storybook/build.static.js",
    "test:unit": "../../node_modules/.bin/jest -c ../../build/config/jest.config.js"
  },
  "files": [
    "dist",
    "src",
    "index.d.ts",
    "build"
  ],
  "author": "Atlassian Pty Ltd",
  "maintainers": [],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/atlassian/atlaskit"
  },
  "devDependencies": {
    "@atlaskit/tooltip": "^8.0.2",
    "@atlaskit/util-readme": "^1.0.0",
    "@atlaskit/util-shared-styles": "*",
    "camelcase": "^3.0.0",
    "classnames": "^2.2.5",
    "clipboard": "^1.5.13",
    "create-error": "^0.3.1",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "javascript-natural-sort": "^0.7.1",
    "less-vars-loader": "^1.1.0",
    "less-vars-to-js": "^1.1.2",
    "prop-types": "^15.5.10",
    "rgba-convert": "^0.3.0",
    "styled-components": "1.4.6 - 3",
    "uid": "0.0.2",
    "wcag-contrast": "^0.1.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "keywords": [
    "atlaskit",
    "ui",
    "internal"
  ]
}
